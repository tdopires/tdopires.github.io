@prefix tds:          <http://example.org/ontology/> .
@prefix rdf:          <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .

tds:MusicGroup rdf:type rdfs:Class .
tds:MusicBand rdfs:subClassOf tds:MusicGroup .
tds:Orchestra rdfs:subClassOf tds:MusicGroup .

tds:Musician rdf:type rdfs:Class .
tds:hasMembers  rdfs:domain	tds:MusicGroup ;
                rdfs:range	tds:Musician .

tds:Concert rdf:type rdfs:Class .
tds:performsAt  rdfs:domain	tds:MusicGroup ;
                rdfs:domain	tds:Musician ;
                rdfs:range	tds:Concert .

tds:MusicAlbum rdf:type rdfs:Class .
tds:releases   rdfs:domain	tds:MusicGroup ;
               rdfs:domain	tds:Musician ;
               rdfs:range 	tds:MusicAlbum .

tds:MusicSong rdf:type rdfs:Class .
tds:containsTrack rdfs:domain	tds:MusicAlbum ;
                  rdfs:range	tds:MusicSong .

tds:MusicGenre rdf:type rdfs:Class .
tds:hasMusicGenre rdfs:domain	tds:MusicSong ;
                  rdfs:range	tds:MusicGenre .

